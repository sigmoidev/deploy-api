pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Init') {
            steps {
                bat './mvnw clean'
            }
        }

        stage('Test') {
            steps {
                bat './mvnw test'
                junit '**/target/surefire-reports/*.xml'
            }
        }

